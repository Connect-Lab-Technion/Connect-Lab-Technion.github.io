keywords = {Journal,MIMO,Multi-agent systems,automatic control,closed loop systems,control system analysis,control system synthesis,convergence,convex functions,couplings,decentralized control,feedback,graph theory,multidimensional systems,nonlinear control systems,nonlinear dynamical systems,optimization},
