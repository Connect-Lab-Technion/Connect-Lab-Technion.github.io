journal = {IEEE Transactions on Automatic Control},
keywords = {Computer architecture,Dynamical systems,Indexes,Journal,Network systems,Passivation,Resilience,Sparsity patterns,Transfer functions,graph theory,matchings,max-flows,passivity},
mendeley-tags = {Journal},
month = {aug},
